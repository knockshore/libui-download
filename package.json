{
  "name": "libui-download",
  "version": "0.1.1",
  "description": "download libui prebuilt binary zips from github releases",
  "main": "es5.js",
  "bin": {
    "libui-download": "cli.js"
  },
  "scripts": {
    "test": "ava -s && xo --ignore es5.js",
    "build": "babel index.js --out-file es5.js"
  },
  "files": [
    "es5.js",
    "cli.js"
  ],
  "repository": "parro-it/libui-download",
  "author": "Andrea Parodi",
  "license": "MIT",
  "dependencies": {
    "debug": "^2.2.0",
    "home-path": "^1.0.1",
    "mkdirp": "^0.5.0",
    "mv": "^2.0.3",
    "pify": "^2.3.0",
    "pinkie-promise": "^2.0.1",
    "rc": "^1.1.2",
    "regenerator-runtime": "^0.9.5",
    "request": "^2.85.0",
    "tar.gz": "^1.0.5"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "rimraf": "^2.5.2",
    "xo": "^0.16.0"
  },
  "keywords": [
    "libui",
    "libui-node",
    "installer"
  ],
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-regenerator",
      "syntax-async-functions"
    ]
  }
}
